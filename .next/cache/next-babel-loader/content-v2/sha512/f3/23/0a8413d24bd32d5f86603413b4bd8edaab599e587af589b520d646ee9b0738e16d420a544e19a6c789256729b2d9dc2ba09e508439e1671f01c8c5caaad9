: [/*#__PURE__*/_jsx(Link, {\n            className: \"sr-only\",\n            href: \"#content\",\n            children: \"Skip to main content\"\n          }), logo ? /*#__PURE__*/_jsx(Link, {\n            className: \"navbar__logo\",\n            href: withPrefix('/'),\n            children: /*#__PURE__*/_jsx(\"img\", {\n              src: withPrefix(logo),\n              alt: logoAlt\n            })\n          }) : /*#__PURE__*/_jsx(Link, {\n            className: \"h4 navbar__title\",\n            href: withPrefix('/'),\n            children: title\n          }), hasNav && !_.isEmpty(navLinks) && this.renderNavLinks(navLinks, pageUrl)]\n        })\n      })\n    });\n  }\n\n}","map":null,"metadata":{},"sourceType":"module"}